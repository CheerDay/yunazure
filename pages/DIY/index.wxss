.imgBox{
  margin-top: 400rpx;
  display: block;
  text-align: center;
}
.Preview{
  width: 350rpx;
  height: 350rpx;
  margin: 0 auto;
  border: 1rpx dashed #666;
}
.TotalView{
  width: 750rpx;
  height: 1000rpx;
}
.backImg{
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.movableIcon{
  width: 100rpx;
  height: 100rpx;
  background-color: rgba(255, 255, 255, 0.164);
  border: solid;
}
.title{
  position: fixed;
  bottom: 100rpx;
} 
.inner{
  position: fixed;
  bottom: 0rpx;
}
.scroll-title{
  width: 750rpx;
  display: inline-block;
  white-space: nowrap;
  text-align: center;
}
.itm{
  margin-left: 2rpx;
  width: 100rpx;
  display: inline-block;
  background: linear-gradient(to bottom right, rgb(168, 235, 247),rgb(248, 198, 244), rgb(168, 235, 247));
  border: 1rpx dashed #666;
}

.pic-box{
  width: 200rpx;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center; 
  position: fixed;
  bottom: 400rpx;
  border-radius: 40rpx;
  margin-left: -55rpx;
  background-color: rgb(255, 255, 255);
  border: 1rpx #000 solid;
  white-space: nowrap;
  padding-left: 0rpx;
}
button::after {
  padding-left: 0rpxs;
}
