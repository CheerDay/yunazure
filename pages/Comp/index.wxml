
<view class="swiper-container" style="height:{{swiperHeight}}rpx;">
  <swiper class="swiper1" indicator-dots="true" indicator-active-color="#fff" autoplay circular style="height:{{swiperHeight}}rpx;">
    <swiper-item wx:for="{{banners}}" wx:key="id" >
      <image  bindtap="tapBanner" data-url="{{item.fileID}}" src="{{item.fileID}}" style="height:{{swiperHeight}}rpx;"  />
    </swiper-item>
  </swiper>
</view>
<button class="commit" hover-class="button-hover" bindtap="commit">提交</button>
